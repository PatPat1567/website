*{box-sizing:border-box}body{font-family:"Lato", sans-serif;line-height:1.4;margin:0;padding:0;width:100%}.wrapper{display:flex;flex-direction:column;width:100%}.hero{background:#0030ff;display:flex;flex-direction:column;height:100vh;min-height:-webkit-fill-available;justify-content:space-between}.hero .intro p{color:white;font-size:24px;font-weight:300}.hero,.principles,footer{padding:30px}.navigation{align-items:center;display:flex;justify-content:space-between}.navigation .logo{fill:white;width:60px}.navigation h1{color:white;font-size:18px}.hero-footer{display:flex;flex-direction:column;justify-content:space-between}.hero-footer p{color:white;font-size:24px;font-weight:900}.hero-footer .social a{text-decoration:none}.hero-footer .social a svg{margin-left:10px}.hero-footer .social a svg .socialIcon{stroke:white}.principles h2{font-size:24px;text-align:center}.principles ul{padding-left:20px}.principles ul li{font-size:18px;margin:1em 0}footer{align-items:center;background:#0030ff;color:white;display:flex;flex-direction:column;justify-content:space-between}@media (min-width: 769px){.hero,.principles{padding:60px}.hero{height:100vh}.hero .intro p{font-size:36px}.hero-footer{flex-direction:row}.principles h2{font-size:32px}.principles ul li{font-size:24px}footer{flex-direction:row;padding:30px 60px}}

