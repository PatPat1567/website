*{box-sizing:border-box}body{font-family:"Lato", sans-serif;margin:0;padding:0;width:100%}.wrapper{display:flex;flex-direction:column;width:100%}.hero{background:blue;display:flex;flex-direction:column;height:100vh;justify-content:space-between}.hero .intro p{color:white;font-size:24px;font-weight:700;line-height:1.4}.hero,.principles,footer{padding:30px}.navigation{align-items:center;display:flex;justify-content:space-between}.navigation .logo{fill:white;width:60px}.navigation h1{color:white;font-size:18px}.hero-footer{display:flex;flex-direction:column;justify-content:space-between}.hero-footer p{color:white;font-size:18px;font-weight:900}.hero-footer .social a{text-decoration:none}.hero-footer .social a svg{margin-left:10px}.hero-footer .social a svg .socialIcon{stroke:white}.principles h2{font-size:2.5em;text-align:center}.principles ul{padding-left:20px}.principles ul li{font-size:1.5em;margin:1em 0}footer{align-items:center;background:blue;color:white;display:flex;flex-direction:column;justify-content:space-between}@media (min-width: 769px){.hero,.principles{padding:60px}.hero{height:600px}.hero .intro p{font-size:36px}.hero-footer{flex-direction:row}footer{flex-direction:row;padding:30px 60px}}

